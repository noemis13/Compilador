digraph G {
	declaracao -> declaracao_variaveis
	declaracao_variaveis -> a
	tipo -> inteiro
	acao -> declaracao_variaveis
	declaracao_variaveis -> ret
	acao -> expressao
	expressao -> atribuicao
	atribuicao -> a
	atribuicao -> fator
	fator -> 10
	acao -> expressao
	expressao -> atribuicao
	atribuicao -> ret
	atribuicao -> fator
	fator -> 1
	acao -> retorna
	retorna -> expressao
	atribuicao -> fator
	fator -> ret
	declaracao_funcao -> principal
	principal -> acao
	principal -> tipo
	declaracao -> declaracao_funcao
}
