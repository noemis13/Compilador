digraph G {
	declaracao -> declaracao_variaveis
	declaracao_variaveis -> a
	tipo -> inteiro
	acao -> declaracao_variaveis
	declaracao_variaveis -> b
	acao -> expressao
	expressao -> atribuicao
	atribuicao -> a
	atribuicao -> fator
	fator -> 10
	acao -> expressao
	expressao -> atribuicao
	atribuicao -> b
	atribuicao -> fator
	fator -> a
	acao -> retorna
	retorna -> expressao
	atribuicao -> fator
	fator -> 0
	declaracao_funcao -> principal
	principal -> acao
	principal -> tipo
	declaracao -> declaracao_funcao
}
